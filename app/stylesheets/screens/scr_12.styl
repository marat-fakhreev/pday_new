#screen-twelve
  .photoalbum
    height: 415px
    width: 100%
    background-color: $blueColor
    position: relative

    img
      height: 415px
      display: inline-block
      float: left
      position: relative
      user-select: none

  div.scrollWrapper
    position: relative
    overflow: hidden
    width: 100%
    height: 100%

  div.scrollableArea
    position: relative
    width: auto
    height: 100%

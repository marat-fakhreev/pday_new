        .circle
          background-image: url('../images/presenters/g_karapetyan.jpg')
        .logo
          background-image: url('../images/logos/15.png')

      .circle-block
        width: 240px
        float: left

        .circle
          width: 207px
          height: 207px
          border-radius: 50%
          background-color: #333
          text-align: center
          position: relative
          left: 50%
          margin: 0 0 20px -120px
          background-position: center 0px
          background-size: cover

          p
            position: absolute
            top: 50px
            font-size: 13px
            width: 100%
            text-align: center
            color: #888

        .logo
          width: auto
          height: 60px
          max-width: 207px
          background-position: center 0px
          background-repeat: no-repeat
          background-size: contain

      .name
        display: block
        text-decoration: none
        font-size: 37px
        font-family: $fontOne
        font-weight: 300
        cursor: default
        margin: 0 0 5px 0

        span
          font-size: inherit
          font-weight: 400
          margin: 0 15px 0 0

        &:hover
          text-decoration: none

      a
        font-size: 15px
        text-decoration: underline

        &:hover
          text-decoration: none

      p
        padding: 15px 0

      .text-block
        padding: 0
        margin: -10px 0 0 267px
        width: 693px

  .partners-list
    li
      .circle-block
        .circle
          border-radius: 0
          background-position: center 0px
          background-repeat: no-repeat
          background-size: contain
          background-color: transparent
          background-image: none
          width: 207px
          height: 144px

          img
            width: auto
            height: auto
            max-width: 207px
            max-height: 144px

        span
          display: block
          font-size: 18px
          text-align: center
          width: 207px

#screen-eight
  .org-list
  .partners-list
    margin: 0 0 50px 0

    li
      padding: 80px 0 80px 0
      width: 100%
      border-bottom: 1px solid $greyColor

      &::after
        content: ''
        display: block
        clear: both

      &:last-child
        border-bottom: none
        padding: 80px 0 0 0

      &:nth-child(1)
        .circle
          background-image: url('../images/presenters/a_sokolov.jpg')
        .logo
          background-image: url('../images/logos/1.png')

      &:nth-child(2)

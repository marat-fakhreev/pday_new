#screen-fourteen
  @extend #screen-six
  background-image: url("../images/bg_six.jpg")

  p
    text-align: left

  h2
    font-size: 72px

  .box
    padding: 40px
    height: auto

    &::after
      content: ''
      display: block
      clear: both

    .time-block
    .map-block
      float: left
      color: #fff
      height: 100%

    .time-block
      width: 185px
      margin: 0 55px 0 0
      float: left

      li
        display: block
        text-align: right
        margin: 0 0 30px 0

        &:last-child
          margin-bottom: 0px

        h2
          position: relative
          left: 8px

        span
          font-size: 24px
          position: relative
          top: -12px

    .map-block
      width: 640px

      h2
        display: block
        margin: 0 0 58px 0

      .icon-pin
        color: $greyDarkColor
        font-size: 16px
        margin: 2px 10px 2px 0

      .g-map
        margin: 70px 0 0 0
        width: 630px
        height: 190px
        background-color: #333

        iframe
          width: 100%
          height: 100%
          border: 0
